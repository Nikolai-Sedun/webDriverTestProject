  </test>
  <test name="File tests">
    <classes>
      <class name="com.itacademy.tests.cloud.FileTest"/>
    </classes>
  </test>
  <test name="Share element tests">
    <classes>
      <class name="com.itacademy.tests.cloud.ShareTest"/>
    </classes>
  </test>
</suite>