<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Testing cloud suite" verbose="2" parallel="tests" thread-count="1"
  allow-return-values="true">
  <test name="Login tests">
    <classes>
      <class name="com.itacademy.tests.cloud.LoginTest"/>
    </classes>
  </test>
  <test name="Folder tests">
    <classes>
      <class name="com.itacademy.tests.cloud.FolderTest"/>
